CLANG_ENABLE_MODULES = YES
CLANG_ENABLE_OBJC_ARC = YES
FRAMEWORK_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/ArcGIS-Runtime-SDK-iOS"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/ArcGIS-Runtime-SDK-iOS"
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited)  -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/ArcGIS-Runtime-SDK-iOS"
OTHER_LDFLAGS = $(inherited) -ObjC -l"c++" -framework "ArcGIS"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}/Pods
